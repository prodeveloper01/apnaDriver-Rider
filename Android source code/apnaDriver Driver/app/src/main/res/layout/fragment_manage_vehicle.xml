<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ColorBackground"
    android:clickable="true"
    android:id="@+id/manage_vehicle_container"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activitiesandfragments.vehiclemanagefragment.ManageVehicleF">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_45sdp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true">

            <ImageView
                android:id="@+id/iv_nav_menu"
                android:layout_width="@dimen/_58sdp"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:paddingStart="@dimen/_16sdp"
                android:paddingEnd="@dimen/_24sdp"
                android:tint="@color/ColorBlack"
                app:srcCompat="@drawable/ic_nav_menu" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_35sdp"
                android:layout_marginEnd="@dimen/_35sdp"
                android:fontFamily="@font/airbnbcerealmedium"
                android:gravity="center"
                android:includeFontPadding="false"
                android:text="@string/manage_vehicle"
                android:textColor="@color/ColorBlack"
                android:textSize="@dimen/_13sdp"
                android:textStyle="normal" />


        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_below="@+id/toolbar"
            android:layout_centerHorizontal="true">


            <RelativeLayout
                android:layout_marginTop="@dimen/_8sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">



                <ImageView
                    android:scaleType="fitStart"
                    android:layout_width="@dimen/_14sdp"
                    android:layout_height="@dimen/_14sdp"
                    android:id="@+id/iv_vehicle_type_arrow"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:tint="@color/ColorDarkGray"
                    app:srcCompat="@drawable/ic_arrow_right"
                    />


                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_toStartOf="@+id/iv_vehicle_type_arrow"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/verhicle_type"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_vehicle_type"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:cursorVisible="false"
                            android:clickable="false"
                            android:focusable="false"
                            android:hint="@string/select_ride_type"
                            android:includeFontPadding="false"
                            android:inputType="textPersonName"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>


                <View
                    android:id="@+id/ll_vehicle_type"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />


            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>



            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">




                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/manufacture"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_manufacture"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:hint="@string/enter_manufacture_company"
                            android:includeFontPadding="false"
                            android:inputType="textCapSentences"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>


            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">




                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/model"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_model"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:hint="@string/enter_model_name"
                            android:includeFontPadding="false"
                            android:inputType="textPersonName"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>


            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">




                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/year"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_year"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:hint="@string/enter_year_of_manufacture"
                            android:includeFontPadding="false"
                            android:inputType="numberSigned"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>




            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">




                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/license_plate"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_lisence_plate"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:hint="@string/enter_lincence_plate"
                            android:includeFontPadding="false"
                            android:inputType="textCapSentences"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>

            <RelativeLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_8sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp">




                <RelativeLayout
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >

                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/color"
                            android:textColor="@color/ColorDarkGray"
                            android:textSize="@dimen/_11sdp"
                            android:textStyle="normal"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            />

                        <EditText
                            android:importantForAutofill="no"
                            android:id="@+id/et_color"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/airbnbcerealmedium"
                            android:gravity="center_vertical"
                            android:hint="@string/enter_vehicle_type"
                            android:includeFontPadding="false"
                            android:inputType="textCapSentences"
                            android:text=""
                            android:paddingTop="@dimen/_2sdp"
                            android:paddingBottom="@dimen/_2sdp"
                            android:background="@null"
                            android:textColor="@color/ColorBlack"
                            android:textColorHint="@color/ColorSemiDarkGray"
                            android:textSize="@dimen/_12sdp"
                            tools:ignore="RtlCompat"
                            android:textAlignment="viewStart"
                            android:textStyle="normal" />


                    </LinearLayout>



                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/ColorSemiDarkGray"/>



        </LinearLayout>


        <LinearLayout
            android:layout_marginBottom="@dimen/_10sdp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:id="@+id/btn_submit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_34sdp"
            android:gravity="center"
            android:background="@drawable/ractengle_less_round_solid_black"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:textAllCaps="false"
                android:text="@string/submit"
                android:textColor="@color/ColorWhite"
                android:textSize="@dimen/_11sdp"
                android:textStyle="normal"
                android:fontFamily="@font/airbnbcerealmedium"
                android:gravity="center"
                />


        </LinearLayout>

    </RelativeLayout>


</FrameLayout>